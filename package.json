{
  "name": "fibra-backend-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "main": "./dist/main.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "1.0.0-rc.8",
    "@nestjs/cli": "7.4.1",
    "@nestjs/common": "7.2.0",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.2.0",
    "@nestjs/mongoose": "7.0.1",
    "@nestjs/platform-express": "7.2.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.14.0",
    "cuid": "2.1.8",
    "helmet": "^3.23.3",
    "ioredis": "4.17.3",
    "lodash": "4.17.15",
    "mongoose": "5.9.20",
    "nest-winston": "1.3.5",
    "nestjs-redis": "^1.2.8",
    "randomstring": "1.1.5",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "rxjs": "6.5.4",
    "source-map-support": "0.5.19",
    "winston": "3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "7.2.0",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.13",
    "@types/mongoose": "5.7.28",
    "@types/node": "13.9.1",
    "@types/supertest": "2.0.7",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/eslint-plugin-tslint": "3.0.2",
    "@typescript-eslint/parser": "3.5.0",
    "commander": "5.1.0",
    "concurrently": "4.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "24.0.0",
    "prettier": "1.19.1",
    "supertest": "3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.18.0",
    "typescript": "3.7.4",
    "wait-on": "3.2.0"
  },
  "eslintIgnore": [
    "*.spec.ts",
    "*.e2e-spec.ts"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "../setupJest.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "collectCoverageFrom": [
      "!**/*.initialization.ts",
      "!**/*.module.ts",
      "!**/base.dao.ts",
      "!**/unitTest.util.ts",
      "!**/redis/*.ts",
      "!**/*.spec.ts",
      "!**/*.e2e-spec.ts",
      "**/*.ts",
      "!**/node_modules/**"
    ],
    "rootDir": "src",
    "testRegex": ".(e2e-)?spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
